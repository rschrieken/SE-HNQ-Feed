doctype xml
rss( version="2.0", xmlns:content="http://purl.org/rss/1.0/modules/content/", xmlns:atom='http://www.w3.org/2005/Atom' )
    channel
        title= title 
        atom:link( href= url rel="self" type="application/rss+xml" )
        link= url
        description My Own Hot Network Questions     
        lastBuildDate= buildDate.toUTCString()
        docs https://lackadaisical-appeal.glitch.me/
        generator https://lackadaisical-appeal.glitch.me/
        
        each feed, i in feeds
            item
                title= feed.title
                link= feed.id
                description= feed.description
                category= feed.category
                guid( isPermaLink="true" )= feed.id
                pubDate= feed.date.toUTCString()
                
                